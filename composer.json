{
  "name": "rockschtar/wordpress-settings",
  "license": "MIT",
  "description": "WordPress Settings Utility/Helper for usage with roots/bedrock (mu-plugin)",
  "type": "wordpress-muplugin",
  "require": {
    "php": ">=8.2",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.9"
  },
  "dist": {
    "type": "zip",
    "url": "https://github.com/rockschtar/wordpress-settings/releases/download/%version%/wordpress-settings-%version%.zip"
  },
  "autoload": {
    "files": [
      "bootstrap.php"
    ],
    "psr-4": {
      "Rockschtar\\WordPress\\Settings\\": "src/",
      "Rockschtar\\WordPress\\Settings\\Tests\\": "tests/"
    }
  }
}
